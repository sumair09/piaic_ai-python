   "source": [
    "# Count Tags In HTML File\n",
    "with open(\"example_html_file.html\", \"r\") as f:\n",
    "    fileContent = f.read()\n",
    "    pairtags = fileContent.count('</')\n",
    "    unpairtags = fileContent.count('/>')\n",
    "    total = pairtags + unpairtags\n",
    "    print(total)"
   ]
